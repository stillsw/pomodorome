<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- ActiveTimer states  -->
    <declare-styleable name="TimerStatefulViews">
        <attr name="timerActive" format="boolean" />
        <attr name="timerPaused" format="boolean" />
        <attr name="timerStopped" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TimePickerCircle">
        <attr name="minutesColour" format="color" />
        <attr name="thumbColour" format="color" />
        <attr name="backgroundColour" format="color" />
        <attr name="divisionsColour" format="color" />
        <attr name="divisionsBackgroundColour" format="color" />
        <attr name="textBackgroundCornerRadius" format="dimension" />
    </declare-styleable>

</resources>